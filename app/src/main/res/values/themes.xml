<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.TP" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/bg_d</item>
        <item name="colorPrimaryVariant">@color/bg_e</item>
        <item name="colorOnPrimary">@color/bg_c</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/bg_i</item>
        <item name="colorSecondaryVariant">@color/bg_f</item>
        <item name="colorOnSecondary">@color/bg_j</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>

        <!--Remove Action Bar-->
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <!-- Customize your theme here. -->
    </style>
</resources>